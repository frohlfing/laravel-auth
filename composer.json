{
    "name": "frohlfing/laravel-auth",
    "description": "User Management for Laravel 5",
    "keywords": ["laravel", "admininstration", "roles", "access control list", "acl", "api-token", "profile", "login", "logout"],
    "license": "MIT",
    "authors": [
        {
            "name": "Frank Rohlfing",
            "email": "mail@frank-rohlfing.de"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "5.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "FRohlfing\\Auth\\": "src/"
        },
        "files": [
            "helpers/auth.php"
        ]
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "laravel": {
            "providers": [
                "FRohlfing\\Auth\\AuthServiceProvider"
            ],
            "aliases": {
                "AuthService": "FRohlfing\\Auth\\Facades\\AuthService"
            }
        }
    }
}
